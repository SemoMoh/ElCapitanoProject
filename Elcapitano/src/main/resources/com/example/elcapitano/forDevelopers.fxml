<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: BLACK;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.elcapitano.ForDevelopers">
   <children>
      <Label alignment="CENTER" layoutX="222.0" layoutY="66.0" text="For Developers" textFill="#fff200">
         <font>
            <Font name="System Bold" size="22.0" />
         </font>
      </Label>
      <TextField fx:id="devusername" alignment="CENTER" layoutX="224.0" layoutY="140.0" promptText="Enter username" />
      <TextField fx:id="devPassword" alignment="CENTER" layoutX="224.0" layoutY="200.0" promptText="Enter Password" />
      <TextField fx:id="pathtoDatabase" alignment="CENTER" layoutX="160.0" layoutY="265.0" prefHeight="26.0" prefWidth="278.0" promptText="Path to database" />
      <Button fx:id="confirmDatabase" layoutX="269.0" layoutY="321.0" mnemonicParsing="false" onAction="#confirmDatabase" text="Confirm" />
   </children>
</AnchorPane>
